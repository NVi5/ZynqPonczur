#include "xparameters.h"
#include <string.h>

#define GPU_MEM ((uint32_t*)XPAR_GPU_CONTROL_0_S00_AXI_BASEADDR)
uint32_t write[10] = {0xffffffff, 0, 1, 2, 3, 4, 5, 6, 7, 8};
uint32_t read[10];

int main()
{
	memcpy(GPU_MEM, write, 40);
	memcpy(read, GPU_MEM, 40);

	while (1);

	return 0;
}